<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>create</servlet-name>
        <servlet-class>com.example.DemoServlet2.controller.CreateStudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>create</servlet-name>
        <url-pattern>/student/create</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>student</servlet-name>
        <servlet-class>com.example.DemoServlet2.controller.GetListStudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>student</servlet-name>
        <url-pattern>/student/list</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>GetListProductServlet</servlet-name>
        <servlet-class>com.example.DemoServlet2.controller.product.GetListProductServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetListProductServlet</servlet-name>
        <url-pattern>/products/list</url-pattern>
        <url-pattern>/products</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CreateProductServlet</servlet-name>
        <servlet-class>com.example.DemoServlet2.controller.product.CreateProductServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateProductServlet</servlet-name>
        <url-pattern>/products/create</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteProductServlet</servlet-name>
        <servlet-class>com.example.DemoServlet2.controller.product.DeleteProductServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteProductServlet</servlet-name>
        <url-pattern>/products/delete</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EditProductServlet</servlet-name>
        <servlet-class>com.example.DemoServlet2.controller.product.EditProductServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditProductServlet</servlet-name>
        <url-pattern>/products/edit</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DetailProductServlet</servlet-name>
        <servlet-class>com.example.DemoServlet2.controller.product.DetailProductServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DetailProductServlet</servlet-name>
        <url-pattern>/products/detail</url-pattern>
    </servlet-mapping>


</web-app>